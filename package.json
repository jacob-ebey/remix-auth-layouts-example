{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "run-s setup:*",
    "setup:prisma": "prisma generate",
    "setup:remix": "remix setup node",
    "build:remix": "remix build",
    "build:tailwind": "tailwindcss -o app/styles/tailwind.css",
    "build:frontend": "run-s build:tailwind build:remix",
    "build:server": "node ./scripts/build-server.js",
    "build": "run-p build:server build:frontend",
    "typecheck": "tsc --noEmit",
    "dev:server": "dotenv ts-node ./server.ts",
    "dev:remix": "dotenv remix watch",
    "dev:tailwind": "tailwindcss -o app/styles/tailwind.css --watch",
    "dev": "npm run build && run-p dev:*",
    "start": "node ./server.js",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@prisma/client": "^3.3.0",
    "@remix-run/express": "^0.19.2",
    "@remix-run/react": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "dinero.js": "^1.9.1",
    "express": "^4.17.1",
    "parse-cache-control": "^1.0.1",
    "prisma": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.6",
    "redis": "^3.1.2",
    "remix": "^0.19.2",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@remix-run/dev": "^0.19.2",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/typography": "^0.4.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/dinero.js": "^1.9.0",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/redis": "^2.8.32",
    "daisyui": "^1.14.4",
    "dotenv-cli": "^4.0.0",
    "esbuild": "^0.12.29",
    "esbuild-node-externals": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false
}
